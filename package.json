{
  "name": "snorreio",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "build:cloudflare": "npm run format:check && npm run lint && npm run build",
    "build:watch": "watch 'npm run build' src",
    "check": "astro check",
    "preview": "astro preview",
    "astro": "astro",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "wrangler:types": "wrangler types",
    "wrangler:dev": "wrangler dev",
    "wrangler:preview": "wrangler pages dev ./dist",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate:local": "wrangler d1 migrations apply snorre-io-prod --local",
    "db:delete:local": "rm $(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit)",
    "db:migrate:prod": "wrangler d1 migrations apply snorre-io-prod",
    "db:migrate:preview": "wrangler d1 migrations apply --env preview snorre-io-preview",
    "db:studio:local": "cross-env LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:preview": "cross-env DB_NAME='snorre-io-preview' WRANGLER_CONFIG='--environment=preview' drizzle-kit studio",
    "db:studio:prod": "drizzle-kit studio"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/cloudflare": "^10.2.5",
    "@astrojs/mdx": "^2.3.1",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.4",
    "@astrojs/solid-js": "^4.1.0",
    "@astrojs/tailwind": "^5.1.0",
    "@atproto/api": "^0.12.7",
    "@cloudflare/workers-types": "^4.20240502.0",
    "@iconify-json/fa": "^1.1.8",
    "@iconify-json/fa6-brands": "1.1.19",
    "@iconify-json/fa6-regular": "1.1.19",
    "@iconify-json/fa6-solid": "1.1.21",
    "@iconify-json/material-symbols": "1.1.79",
    "@iconify-json/mdi": "^1.1.66",
    "@kobalte/core": "^0.13.1",
    "@kobalte/tailwindcss": "^0.9.0",
    "astro": "^4.7.1",
    "astro-icon": "^1.1.0",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.30.10",
    "jose": "^5.2.4",
    "rehype-infer-description-meta": "^2.0.0",
    "sharp": "^0.33.3",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.17",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@resvg/resvg-js": "^2.6.2",
    "@types/node": "^20.12.8",
    "astro-robots-txt": "^1.0.0",
    "better-sqlite3": "^9.6.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.17",
    "node-html-parser": "^6.1.13",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "satori": "^0.10.13",
    "satori-html": "^0.3.2",
    "solid-devtools": "^0.30.1",
    "watch": "^0.13.0"
  }
}
