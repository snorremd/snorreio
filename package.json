{
  "name": "snorreio",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "build:cloudflare": "npm run format:check && npm run lint && npm run build",
    "build:watch": "watch 'npm run build' src",
    "check": "astro check",
    "preview": "astro preview",
    "astro": "astro",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "wrangler:types": "wrangler types",
    "wrangler:dev": "wrangler dev",
    "wrangler:preview": "wrangler pages dev ./dist",
    "db:codegen": "kysely-codegen --dialect=sqlite --exclude-pattern \"(_CfKV|D1Migrations)\" --url $(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit)",
    "db:migrate:local": "wrangler d1 migrations apply snorre-io-prod --local",
    "db:delete:local": "rm $(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit)",
    "db:migrate:prod": "wrangler d1 migrations apply snorre-io-prod --remote",
    "db:migrate:preview": "wrangler d1 migrations apply --env preview snorre-io-preview --remote",
    "db:studio:local": "cross-env LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:preview": "cross-env DB_NAME='snorre-io-preview' WRANGLER_CONFIG='--environment=preview' drizzle-kit studio",
    "db:studio:prod": "drizzle-kit studio"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "12.6.2",
    "@astrojs/mdx": "4.3.3",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/rss": "4.0.12",
    "@astrojs/sitemap": "3.4.2",
    "@astrojs/solid-js": "5.1.0",
    "@astrojs/tailwind": "6.0.2",
    "@atproto/api": "^0.13.15",
    "@cloudflare/workers-types": "^4.20241106.0",
    "@iconify-json/fa": "^1.2.0",
    "@iconify-json/fa6-brands": "1.2.1",
    "@iconify-json/fa6-regular": "1.2.1",
    "@iconify-json/fa6-solid": "1.2.1",
    "@iconify-json/material-symbols": "1.2.6",
    "@iconify-json/mdi": "^1.2.1",
    "@kobalte/core": "^0.13.7",
    "@kobalte/tailwindcss": "^0.9.0",
    "astro": "5.12.8",
    "astro-icon": "^1.1.2",
    "date-fns": "^4.1.0",
    "jose": "^5.9.6",
    "kysely": "^0.27.4",
    "kysely-d1": "^0.3.0",
    "rehype-infer-description-meta": "^2.0.0",
    "solid-icons": "^1.1.0",
    "solid-js": "1.9.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@resvg/resvg-js": "^2.6.2",
    "@types/node": "^22.9.0",
    "astro-robots-txt": "^1.0.0",
    "better-sqlite3": "^11.5.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.28.0",
    "kysely-codegen": "^0.17.0",
    "node-html-parser": "^6.1.13",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "satori": "^0.11.3",
    "satori-html": "^0.3.2",
    "solid-devtools": "^0.30.1",
    "watch": "^1.0.2"
  },
  "trustedDependencies": ["@biomejs/biome"]
}
